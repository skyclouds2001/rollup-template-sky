{
  "name": "rollup-template-sky",
  "private": true,
  "version": "1.6.0",
  "description": "An opinionated rollup starter template",
  "keywords": [
    "template",
    "rollup",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "browser": "./dist/index.umd.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "browser": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyclouds2001/rollup-template-sky.git"
  },
  "homepage": "https://github.com/skyclouds2001/rollup-template-sky#readme",
  "bugs": {
    "url": "https://github.com/skyclouds2001/rollup-template-sky/issues",
    "email": "skyclouds2001@163.com"
  },
  "license": "MIT",
  "author": {
    "name": "skyclouds2001",
    "email": "skyclouds2001@163.com",
    "url": "https://skyclouds2001.github.io"
  },
  "contributors": [
    "skyclouds2001"
  ],
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/skyclouds2001"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0",
    "yarn": ">=1.22.22",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rollup --config",
    "preview": "vite ./playground",
    "lint": "cross-env ESLINT_CONFIG_PRETTIER_NO_DEPRECATED=0 && eslint --fix . && prettier --write . && markdownlint --fix **/*.md && tsc --noEmit",
    "test": "cross-env NODE_ENV=test vitest && playwright test",
    "clean": "rimraf node_modules && rimraf dist",
    "changeset": "changeset",
    "bump": "changeset version",
    "commit": "cz",
    "prepare": "is-ci || husky",
    "preinstall": "only-allow pnpm"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@playwright/test": "^1.48.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@rollup/plugin-virtual": "^3.0.2",
    "@rollup/plugin-wasm": "^6.2.2",
    "@rollup/plugin-yaml": "^4.1.2",
    "@types/node": "^22.8.1",
    "@vitest/coverage-v8": "^2.1.3",
    "@vitest/ui": "^2.1.3",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-playwright": "^2.0.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.10",
    "markdownlint": "^0.36.0",
    "markdownlint-cli": "^0.42.0",
    "only-allow": "^1.2.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.2",
    "tslib": "^2.8.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.3"
  }
}
